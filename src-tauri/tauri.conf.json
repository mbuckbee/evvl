{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Evvl",
  "version": "0.1.6",
  "identifier": "com.evvl.app",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3333",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build:tauri"
  },
  "app": {
    "windows": [
      {
        "title": "Evvl",
        "width": 1400,
        "height": 900,
        "minWidth": 1000,
        "minHeight": 700,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "signingIdentity": null,
      "hardenedRuntime": true
    },
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://app.evvl.ai/api/updates/check"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEIxODRFMTE3Mzc1RUMwRUYKUldUdndGNDNGK0dFc1RaYkdJWTE2U2gxSW0vZWFKWjJvQ0hCeHI2bTRMQVpmSkY5OC8weE5vVkcK"
    }
  }
}
